{
  "expo": {
    "name": "SoGoJet",
    "slug": "SoGoJet",
    "version": "1.0.0",
    "scheme": "sogojet",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "description": "Swipe through incredible travel deals. Discover destinations matched to your style and budget.",
    "githubUrl": "https://github.com/jacksoncamp/SwypeFly",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sogojet.app",
      "infoPlist": {
        "NSCameraUsageDescription": "SoGoJet needs camera access to scan boarding passes.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F172A"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sogojet.app",
      "permissions": ["INTERNET", "ACCESS_NETWORK_STATE"]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": ""
      }
    },
    "updates": {
      "url": ""
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
